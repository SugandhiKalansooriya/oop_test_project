	.loader-wrapper.hiide-loeader{
		opacity: 0;
		pointer-events: none;
	}

	.loader-wrapper .loading img{
		width: 200px;
		animation: heartbeat .8s alternate infinite ease-in;
		filter: grayscale(100%);
		opacity: .2;

	}

	.loader-wrapper .wrap:before{
		content: '';
		position: absolute;
		top: 0;
		left: 0;
		background: transparent;
		width: calc(100% - 20px);
		height: calc(100% - 20px);
	} 

